{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "Storage": {
      "AzureBlob": {
        "Media": {
          "ConnectionString": "DefaultEndpointsProtocol=https;AccountName=umbmultisitestorage;AccountKey=OulUjc1wclzGU/IjDXwTDMppqnXx9bATkPbg0BUqmwv6JW1SHHfrCsAIuH+GQqvw7iu1i4bJJ7Cl+AStNY/myQ==;EndpointSuffix=core.windows.net",
          "ContainerName": "umbstorage"
        }
      }
    },
    "CMS": {
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto",
        "ModelsNamespace": "Wmca.Models.Content",
        "ModelsDirectory": "~/../Wmca.Models/Content",
        "AcceptUnsafeModelsDirectory": true
      },
      "Global": {
        "Id": "84d24f14-acec-4415-b91d-d278522e438d",
        "SanitizeTinyMce": true,
        "MainDomLock": "FileSystemMainDomLock"
      },
      "Hosting": {
        "LocalTempStorageLocation": "EnvironmentTemp"
      },
      "Examine": {
        "LuceneDirectoryFactory": "SyncedTempFileSystemDirectoryFactory"
      },
      "Content": {
        "Error404Collection": [
          {
            "Culture": "default",
            "ContentXPath": "//error[@nodeName='Error']"
          }
        ],
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "DeliveryApi": {
        "Enabled": true,
        "PublicAccess": false,
        "ApiKey": ""
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=(LocalDb)\\MSSQLLocalDB;Initial Catalog=TestDB2;Integrated Security=True",
    "umbracoDbDSN_ProviderName": "System.Data.SqlClient"
  },
  "uSync": {
    "Publisher": {
      "Settings": {
        "IncomingEnabled": true,
        "AppId": "",
        "AppKey": ""
      }
    }
  },
  "AzureSSO": {
    "Credentials": {
      "Instance": "https://login.microsoftonline.com/",
      "Domain": "",
      "TenantId": "",
      "ClientId": "",
      "CallbackPath": "/umbraco-microsoft-signin/",
      "SignedOutCallbackPath ": "/umbraco-microsoft-signout/",
      "ClientSecret": ""
    },
    "DisplayName": "your corporate account",
    "DenyLocalLogin": false,
    "AutoRedirectLoginToExternalProvider": false,
    "TokenCacheType": "InMemory",
    "GroupBindings": {
      "<AD group>": "umbraco"
    },
    "SetGroupsOnLogin": false,
    "Icon": "fa fa-lock",
    "ButtonStyle": "btn-microsoft"
  }
}
